{
	"name": "pkg-name",
	"version": "0.0.0",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"description": "",
	"exports": {
		"types": "./dist/index.d.ts",
		"import": "./dist/index.js",
		"require": "./dist/index.cjs"
	},
	"scripts": {
		"build": "vite build",
		"dev": "vite build --watch",
		"prepublishOnly": "bun run build",
		"test": "bun test",
		"test:coverage": "bun test --coverage",
		"lint": "eslint --config .config/eslint.config.ts .",
		"lint:fix": "eslint --config .config/eslint.config.ts . --fix",
		"biome": "biome check .",
		"biome:fix": "biome check --write .",
		"knip": "knip",
		"knip:fix": "knip --fix",
		"knip:production": "knip --production",
		"knip:watch": "knip --watch",
		"check": "bun run lint && bun run biome && bun run knip",
		"check:fix": "bun run lint:fix && bun run biome:fix && bun run knip:fix"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"bun"
	],
	"license": "MIT",
	"homepage": "https://github.com/samhoque/pkg-name#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/samhoque/pkg-name.git"
	},
	"bugs": "https://github.com/samhoque/pkg-name/issues",
	"author": "Sam Hoque <samzsakerz@gmail.com>",
	"devDependencies": {
		"@biomejs/biome": "2.3.10",
		"@commitlint/cli": "^20.2.0",
		"@commitlint/config-conventional": "^20.2.0",
		"@commitlint/types": "^20.2.0",
		"@eslint/js": "^9.39.2",
		"@types/bun": "^1.3.5",
		"commitlint-config-gitmoji": "^2.3.1",
		"eslint": "^9.39.2",
		"eslint-plugin-sonarjs": "^3.0.5",
		"globals": "^16.5.0",
		"jiti": "^2.6.1",
		"knip": "^5.78.0",
		"lefthook": "^2.0.13",
		"typescript-eslint": "^8.51.0",
		"vite": "^7.3.0",
		"vite-plugin-dts": "^4.5.4"
	}
}
